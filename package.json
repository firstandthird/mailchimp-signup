{
  "name": "@firstandthird/mailchimp-signup",
  "version": "1.1.1",
  "description": "Mailchimp subscribe simple UI",
  "main": "dist/mailchimp-signup.js",
  "module": "lib/mailchimp-signup",
  "scripts": {
    "start": "scriptkit",
    "dev": "scriptkit dev",
    "build": "scriptkit build",
    "test": "cat test/mailchimp-signup.test.dist.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublishOnly": "scriptkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/mailchimp-signup.git"
  },
  "keywords": [
    "dom",
    "browser",
    "module",
    "helper"
  ],
  "author": "First+Third <code@firstandthird.com> (http://firstandthird.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/mailchimp-signup/issues"
  },
  "homepage": "https://github.com/firstandthird/mailchimp-signup#readme",
  "devDependencies": {
    "eslint-config-firstandthird": "^4.3.0",
    "eslint-plugin-import": "^2.9.0",
    "phantomjs-prebuilt": "^2.1.14",
    "scriptkit": "^0.3.0",
    "static-server": "^3.0.0",
    "tap-spec": "^4.1.1",
    "tape-rollup": "^4.6.4",
    "tape-run": "^3.0.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird",
    "rules": {
      "no-underscore-dangle": 0
    }
  },
  "scriptkit": {
    "files": {
      "dist/mailchimp-signup.js": "lib/mailchimp-signup",
      "test/mailchimp-signup.test.dist.js": "test/mailchimp-signup.test.js"
    },
    "test": {
      "timeout": false
    }
  },
  "dependencies": {
    "@firstandthird/jsonp": "^1.0.0",
    "domassist": "^1.9.0",
    "domodule": "^5.1.2",
    "formobj": "^4.0.1"
  },
  "directories": {
    "test": "test"
  }
}
